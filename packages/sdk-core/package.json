{
  "name": "@thirdweb-dev/sdk-core",
  "version": "0.0.0",
  "description": "Experimental thirdweb SDK core",
  "repository": "https://github.com/thirdweb-dev/js/tree/main/packages/sdk",
  "license": "Apache-2.0",
  "main": "dist/thirdweb-dev-sdk-core.cjs.js",
  "module": "dist/thirdweb-dev-sdk-core.esm.js",
  "browser": {
    "./dist/thirdweb-dev-sdk-core.esm.js": "./dist/thirdweb-dev-sdk-core.browser.esm.js"
  },
  "exports": {
    ".": {
      "module": {
        "browser": "./dist/thirdweb-dev-sdk-core.browser.esm.js",
        "default": "./dist/thirdweb-dev-sdk-core.esm.js"
      },
      "default": "./dist/thirdweb-dev-sdk-core.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ],
    "exports": {
      "envConditions": [
        "browser"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint src/",
    "fix": "eslint src/ --fix",
    "clean": "rm -rf dist/",
    "build": "tsc && preconstruct build"
  },
  "devDependencies": {
    "@preconstruct/cli": "^2.2.1",
    "@thirdweb-dev/tsconfig": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-thirdweb": "workspace:*",
    "eslint-plugin-better-tree-shaking": "0.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-inclusive-language": "^2.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.16",
    "prettier": "^2.7.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "viem": "^1.2.0",
    "@thirdweb-dev/chains": "workspace:*",
    "@thirdweb-dev/contracts-js": "workspace:*"
  },
  "bugs": {
    "url": "https://github.com/thirdweb-dev/js/issues"
  },
  "author": "thirdweb eng <eng@thirdweb.com>"
}
